{
  "name": "frens-express-graphql-api",
  "version": "1.2.0",
  "description": "A graphql api for a simple social feed with prisma and express",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "vercel-build": "echo uwu",
    "tsup:build": "npx prisma generate && tsup src/index.ts --format cjs --sourcemap --out-dir dist",
    "tsup:start": "node ./dist/index.js",
    "build": "npx run tsup:build",
    "start": "npx run tsup:start",
    "prebuild": "npm run db:prod",
    "tsc:build": "tsc && tsc-alias",
    "tsc:start": "tsc && tsc-alias",
    "start:tsx": "node dist/src/index.ts",
    "lint": "eslint --fix src test",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:prod": "prisma migrate deploy && prisma generate --no-engine",
    "db:migrate": "prisma migrate dev --skip-generate",
    "db:studio": "prisma studio",
    "gen:bulk": "tsx src/api/admin.ts"
  },
  "keywords": [],
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "dependencies": {
    "@pothos/core": "^4.5.1",
    "@pothos/plugin-prisma": "^4.5.0",
    "@pothos/plugin-relay": "^4.4.1",
    "@prisma/client": "^6.5.0",
    "better-auth": "^1.2.5",
    "cookie-parser": "^1.4.7",
    "graphql": "^16.10.0",
    "graphql-yoga": "^5.13.2",
    "zod": "^3.24.2",
    "request-ip": "^3.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.24.0",
    "prisma": "^6.5.0",
    "supertest": "^7.1.0",
    "tsc-alias": "^1.8.13",
    "tsup": "^8.4.0"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
